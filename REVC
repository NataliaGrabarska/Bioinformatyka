DNA = 'AAAACCCGGT'        #łańcuch DNA podany w zadaniu

iDNA = DNA[::-1]          #tworzenie zmiennej iDNA która odwraca kolejność znaków w zmiennej DNA

komp_zasady = {'A':'T', 'T':'A', 'C':'G', 'G':'C'}      #tworzenie słownika w którym każdej zasadzie azotowej przypisuje się komplementarną jej zasadę

for i in iDNA:                              #pętla for działająca na zmiennej iDNA, która korzystając z danych w słowniku zmienia dany klucz na podaną wartość
    print(komp_zasady[i], end = '')
