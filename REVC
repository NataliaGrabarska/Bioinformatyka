DNA = 'AAAACCCGGT'        #DNA string from the exercise

iDNA = DNA[::-1]          #'iDNA' with changed order of characters in the DNA string

complement = {'A':'T', 'T':'A', 'C':'G', 'G':'C'}      #dictionary with complementary bases

for i in iDNA:                              #for loop that changes nucleobases in the DNA string using 'complement' dictionary
    print(complement[i], end = '')
