s = 'GATATATGCATATACTT'
t = 'ATAT'                  #łańcuchy DNA podane w zadaniu

def położenie(s,t):         #funkcja 'położenie(s,t)' podająca miejsce położenia łańcucha t w łańcuchu s
    wynik = []
    
    lt = len(t)             #zmienna której przypisana jest długość łańcucha t
    
    for i in range(len(s)-lt):        #pętla for z warunkiem if który zostaje spełniony gdy w łańcuchu s pojawi się łańcuch t
        if s[i:i+lt] == t:
            wynik.append(i+1)         #wynik pętli który wskazuje miejsce pierwszego znaku łańcucha t w łańcuchu s
    return wynik

print(położenie(s,t))           #wywołanie funkji 'położenie(s,t)'
